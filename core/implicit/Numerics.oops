final class Numerics {

    public static def signum<N>(N num) -> N {
        return num < 0 ? -1 : num > 0 ? 1 : 0;
    }

    public static def positiveMedian(int start, int end) -> int {
        return (start + end) >>> 1;
    }

    public static def positiveMedian(long start, long end) -> long {
        return (start + end) >>> 1;
    }
}